{
  "expo": {
    "name": "TwitchTVClone",
    "slug": "TwitchTVClone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#bf94ff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.r3nanp.twitchappclone"
    },
    "android": {
      "package": "com.r3nanp.twitchappclone"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
